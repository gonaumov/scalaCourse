object Sheet7 {
  def isVomel(ch: Char): Boolean = {              //> Welcome to the Scala worksheet
  	"aeiou".contains(ch)
  }                                               //> isVomel: (ch: Char)Boolean
  
  def vomels(s: String): String = {
      for(c <- s if isVomel(c)) yield c
  }          
}