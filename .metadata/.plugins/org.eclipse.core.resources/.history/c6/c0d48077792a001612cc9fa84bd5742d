object Sheet2 {
	println("hui")                            //> hui
	import scala.math._
	sqrt(10)                                  //> res0: Double = 3.1622776601683795
	1.to(10)                                  //> res1: scala.collection.immutable.Range.Inclusive = Range(1, 2, 3, 4, 5, 6, 7,
  1.to(10).map(sqrt(_))                                //|  8, 9, 10)
}