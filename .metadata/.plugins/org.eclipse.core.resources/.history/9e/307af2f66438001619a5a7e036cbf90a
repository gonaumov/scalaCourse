object Sheet1 {
  println("Welcome to the Scala worksheet")
  
  def swapTouple(input: (Int, Int)): (Int, Int) = {
  	input match {
  		case (x, y) => (y, x)
  	}
  }
  
  swapTouple((2,1))
  
  
  def swap(input: Array[Int]): Array[Int] = {
  	input match {
  		case (a,b, rest @ _*) => Array(b,b) += _*
  	}
  }
}