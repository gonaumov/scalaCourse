object Sheet5 {
  println("Welcome to the Scala worksheet")
  def isVomel(ch: Char): Boolean = {
  	"aeiou".contains(ch)
  }
  
  def vomels(s: String): String = {
  		var result: String = "";
  		for(c <- s) if(isVomel(c)) result.concat(c.toString())
  		
  		result
  }
  
  vomels("alabalanica")
}