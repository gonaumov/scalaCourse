  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  val x = -1                                      //> x  : Int = -1
  
  val result = if(x > 0) "something"              //> result  : Any = ()
  
  val result2 = ()                                //> result2  : Unit = ()
  
  for(i <- 1 to n) println(i)
}